<h1>Listing vaccinations</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Type</th>
    <th>Date</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vaccinations.each do |vaccination| %>
  <tr>
    <td><%= vaccination.title %></td>
    <td><%= vaccination.vacination_type %></td>
    <td><%= vaccination.date %></td>
    <td><%= vaccination.description %></td>
    <td><%= link_to 'Show', vaccination %></td>
    <td><%= link_to 'Edit', edit_vaccination_path(vaccination) %></td>
    <td><%= link_to 'Destroy', vaccination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vaccination', new_vaccination_path %>
